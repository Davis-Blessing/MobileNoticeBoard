        <activity
            android:name=".View_Or_Post_Notice"
            android:label="@string/title_activity_view__or__post__notice" >
        </activity>
        <activity
            android:name=".Post_Notice"
            android:label="@string/title_activity_post__notice" >
        </activity>

    </application>

</manifest>
